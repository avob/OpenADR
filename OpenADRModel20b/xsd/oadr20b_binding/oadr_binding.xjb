		</schemaBindings>
		<bindings>
			<namespace:prefix name="xml" />
		</bindings>
	</bindings>
	<bindings schemaLocation="../oadr20b_schema/oadr_xmldsig-properties-schema.xsd">
		<schemaBindings>
			<package name="com.avob.openadr.model.oadr20b.xmldsig.properties" />
		</schemaBindings>
		<bindings>
			<namespace:prefix name="properties" />
