			<package name="com.avob.openadr.model.oadr20b.iso" />
		</schemaBindings>
		<bindings>
			<namespace:prefix name="iso" />
		</bindings>
	</bindings>
	<bindings schemaLocation="../oadr20b_schema/oadr_power_20b.xsd">
