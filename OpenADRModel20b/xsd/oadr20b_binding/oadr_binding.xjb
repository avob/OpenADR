		</schemaBindings>
		<bindings>
			<namespace:prefix name="xmldsig11" />
		</bindings>
	</bindings>
	<bindings schemaLocation="../oadr20b_schema/oadr_avob.xsd">
		<schemaBindings>
			<package name="com.avob.openadr.model.oadr20b.avob" />
		</schemaBindings>
		<bindings>
			<namespace:prefix name="avob" />
		</bindings>
	</bindings>	
</bindings>

