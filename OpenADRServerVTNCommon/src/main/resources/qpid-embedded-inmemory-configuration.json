{
	"name": "Embedded Broker",
	"modelVersion": "7.0",
	"authenticationproviders": [
		{
			"name": "hardcoded",
			"type": "Plain",
			"secureOnlyMechanisms": [],
			"users": [
				{
					"name": "guest",
					"password": "guest",
					"type": "managed"
				}
			]
		}
	],
	"ports": [
		{
			"name": "AMQP",
			"port": "${qpid.amqp_port}",
			"bindingAddress": "127.0.0.1",
			"protocols": [
				"AMQP_0_9_1"
			],
			"authenticationProvider": "hardcoded",
			"virtualhostaliases": [
				{
					"name": "defaultAlias",
					"type": "defaultAlias"
				}
			]
		}
	],
	"virtualhostnodes": [
		{
			"name": "default",
			"type": "Memory",
			"defaultVirtualHostNode": "true",
			"virtualHostInitialConfiguration": "{\"type\": \"Memory\"}"
		}
	]
}